package day04;

public class GraphicDesc {
	public static void main(String[] args) {
		print2(18);
		
	}
	
	public static void print1(int row){
		for(int i = 1;i<=row;i++){
			for(int j = 1;j<=i;j++){
				System.out.print("#");
			}
			System.out.println();
		}
	}
	public static void print2(int row){
		for(int i = 1;i<=row;i++){
			for(int j = 1;j<=row-i+1;j++){
				System.out.print("#");
			}
			System.out.println();
		}
	}
}
